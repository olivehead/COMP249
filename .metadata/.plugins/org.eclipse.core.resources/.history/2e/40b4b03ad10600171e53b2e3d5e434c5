
public class Book {
	protected long ISBN;
	protected String title;
	protected int issueYear;
	protected String authorName;
	protected double price;
	protected int numberOfPages;
	
	public Book() {
		ISBN = 0;
		title = null;
		issueYear=0;
		authorName=null;
		price=0.0;
		numberOfPages=0;
	}
	public Book() {
		
	}

}
